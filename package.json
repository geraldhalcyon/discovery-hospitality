    "@next/bundle-analyzer": "^13.4.19",
    "@release-it/conventional-changelog": "^8.0.1",
    "axios": "^1.2.2",
    "dotenv": "^16.0.3",
    "eslint": "8.31.0",
    "eslint-config-next": "13.1.1",
    "fs": "^0.0.1-security",
    "husky": "^9.0.11",
    "jsona": "^1.11.0",
    "moment-timezone": "^0.5.41",
